{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        
        "applicationName": {
            "value": "<YOUR_APPLICATION_NAME>"
            
            
        },
        "svcPlanName": {
            "value": "<YOUR_APPLICATION_PLAN_NAME>"
                    
        },    
        "sku": {
            "value": "Standard"
            
        },    
        "sqlServerName" : {
            "value": "<YOUR_SQL_SERVER_NAME>"
            
        },
        "administratorLogin": {
            "value": "<ADMIN_LOGIN>"
            
        },
        "administratorLoginPassword": {
            "reference": {
                "keyVault": {
                    "id": "YOUR_KEYVAULT"
                },
                "secretName": "<KEYVAULT_SECRET>"
            }
            
        },
        "sqldbName" : {
            "value": "<YOUR_SQL_DB_NAME>"
            
        },
        "autoscaleEnabled": {
            "value": true
            
        },
        "svcPlanSize": {
            "value": "S1"
            
        },
        "minimumCapacity": {
            "value": 2
            
        },
        "maximumCapacity": {
            "value": 5
            
        },
        "defaultCapacity": {
            "value": 5
            
        },
        "metricName": {
            "value": "CpuPercentage"
            
        },
        "metricThresholdToScaleOut": {
            "value": 60
            
        },
        "metricThresholdToScaleIn": {
            "value": 20
            
        },
        "changePercentScaleOut": {
            "value": 20
            
        },
        "changePercentScaleIn": {
            "value": 10
            
        }
           
        
        
    }
}